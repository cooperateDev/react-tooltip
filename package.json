{
  "name": "react-tooltip",
  "version": "0.1.7",
  "description": "react tooltip component",
  "main": "dist/react-tooltip.js",
  "scripts": {
    "test": "jest",
    "build:js": "watchify src/js/index.jsx -t babelify -o dist/react-tooltip.js -dv ",
    "build:css": "node-sass -w --output-style compressed src/scss/style.scss dist/react-tooltip.css",
    "dev": "npm run build:js & npm run build:css"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/__tests__/preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wwayne/react-tooltip"
  },
  "keywords": [
    "react",
    "react-component",
    "tooltip",
    "react-tooltip"
  ],
  "author": "wwayne",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wwayne/react-tooltip/issues"
  },
  "homepage": "https://github.com/wwayne/react-tooltip",
  "peerDependencies": {
    "react": ">=0.12.0"
  },
  "dependencies": {
    "classnames": "^1.2.0"
  },
  "devDependencies": {
    "babelify": "^6.0.2",
    "jest": "^0.1.39",
    "jest-cli": "^0.4.0",
    "live-reload": "^1.1.0",
    "node-sass": "^3.0.0",
    "react": "^0.13.1",
    "react-tools": "^0.13.2",
    "watchify": "^3.2.1"
  }
}
